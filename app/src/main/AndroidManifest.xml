<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zr.wanandroid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/android"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/android"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".test.activity.MainActivity"></activity>
        <activity android:name=".test.activity.TestActivity" />
        <activity android:name=".test.activity.Test2Activity" />
        <activity android:name=".test.activity.EmptyActivity" />
        <activity android:name=".test.activity.Test3Activity" />
        <activity
            android:name=".module.home.activity.HomeActivity"
            android:configChanges="layoutDirection|orientation|keyboardHidden|keyboard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".module.web.act.WebActivity"
            android:configChanges="layoutDirection|orientation|keyboardHidden|keyboard" />
    </application>

</manifest>